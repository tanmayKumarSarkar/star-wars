<%include partials/header%>


<div id="hero-image">
  <h1 id="hero-title">my star wars App</h1>
</div>
<div id="posters-wrapper" class="content-padding clearfix">
  <% movies.forEach((movie)=>{%>
    <div class="poster">
      <a href="/star_wars_episode/<%=movie.episode_number%>">
      <img src="/images/<%=movie.poster%>" class="img-responsive">
        <div class="poster-info-overlay">
          <h3><%=movie.title%></h3>
          <h4>View More <i class="fa fa-arrow-right"></i></h4>
        </div>
        </a>
    </div>
  <%});%>
</div>
</div>



<%include partials/footer%>
