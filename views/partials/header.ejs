<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/stylesheets/styles.css" media="screen" charset="utf-8">
    <title>Star Wars Home Page</title>
  </head>
  <body>
    <nav class="Navbar navbar-fixed-top">
      <div class="content-padding">
          <ul class="Navbar navbar-nav navbar-right">
            <li><a href="/">Home</a></li>
            <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Movies <i class="fa fa-chevron-down"></i></a>
              <ul class="dropdown-menu">
                <% movies.forEach((movie)=>{%>
                  <li><a href="/star_wars_episode/<%=movie.episode_number%>" class="episode_link"><%=movie.title%></a></li>
                <%});%>
              </ul>
            </li>
          </ul>
      </div>
    </nav>
